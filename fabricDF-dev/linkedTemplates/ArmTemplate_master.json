{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fabricDF-dev"
		},
		"ls_blobfabricstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobfabricstorage'"
		},
		"ls_demo_fabric_sqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_demo_fabric_sqlserver'"
		},
		"ls_demo_fabric_sqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "demo-fabric-sqlserver.database.windows.net"
		},
		"ls_demo_fabric_sqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "fabric_database"
		},
		"ls_demo_fabric_sqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azureadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fabricDF-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blobfabricstorage_connectionString": {
						"value": "[parameters('ls_blobfabricstorage_connectionString')]"
					},
					"ls_demo_fabric_sqlserver_password": {
						"value": "[parameters('ls_demo_fabric_sqlserver_password')]"
					},
					"ls_demo_fabric_sqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_demo_fabric_sqlserver_properties_typeProperties_server')]"
					},
					"ls_demo_fabric_sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_demo_fabric_sqlserver_properties_typeProperties_database')]"
					},
					"ls_demo_fabric_sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_demo_fabric_sqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "fabricDF-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'fabricDF-dev_ArmTemplate_0')]"
			]
		}
	]
}